name: latihan-cs-mlflow

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 505}
      max_depth: {type: int, default: 35}
      train_dataset: {type: string, default: "dataset/train_dataset.csv"}
      test_dataset: {type: string, default: "dataset/test_dataset.csv"}
    command: "python modelling.py {n_estimators} {max_depth} {train_dataset} {test_dataset}"
